%-----------EXPERIENCE-----------
\section{Experience}
  \resumeSubHeadingListStart
    {% for experience in experiences %}
    \resumeSubheading
      {{ '{' }}{{ experience.title | le }}{{ '}' }}{{ '{' }}{{ experience.duration | le }}{{ '}' }}
      {{ '{' }}{{ experience.company | le }}{{ '}' }}{{ '{' }}{{ experience.location | le }}{{ '}' }}
      \resumeItemListStart
        {% for item in experience.descriptions %}
        \resumeItem{{ '{' }}{{ item | le }}{{ '}' }}
        {% endfor %}
      \resumeItemListEnd
    {% endfor %}
  \resumeSubHeadingListEnd
